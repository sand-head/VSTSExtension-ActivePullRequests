    "mini-css-extract-plugin": "^2.5.3",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "sass": "^1.49.7",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "tfx-cli": "^0.10.0",
