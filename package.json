{
  "scripts": {
    "lint": "tslint -c tslint.json 'src/*.ts'",
    "serve": "webpack serve --open --config webpack.serve.js",
    "build": "npm run lint && webpack --config webpack.build.js",
    "postbuild": "npm run package",
    "package": "tfx extension create --rev-version --manifest-globs ./mimeo-devops-extension.json --overrides-file ./configs/release.json --output-path ./ext",
